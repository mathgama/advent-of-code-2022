{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "prettier/prettier": 2
    }
}
